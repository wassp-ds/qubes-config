base:
  'dom0':
    - match: nodegroup
    - otee-dev.create
  'tpl-otee-dev':
    - otee-dev.install
  'dvm-otee-dev':
    - otee-dev.configure-dvm
  'otee-dev':
    - otee-dev.configure
  '(I@qubes:type:template or I@qubes:type:standalone) and (G@kernel:Linux or G@kernel:*BSD)':
    - match: compound
    - sys-net.install-proxy
